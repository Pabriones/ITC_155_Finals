
public class Question_3 {

	public static void main(String[] args) {
		
		Integer[] num = {3,5,6};
		System.out.println(isSorted(num));

	}
	public static boolean isSorted(Integer[] array) {
	    int pre = array[0];
	    
	    for (int i = 1; i < array.length; i++) {
	        int current = array[i];
	        if (pre >= current) {
	            return false;
	        }
	        pre = current;
	    }
	    
	    return true;
	}

}
