import java.util.*;

public class runtime {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		System.out.println(ntime());
    //Answer is O(N^2)
	}
	
	public static void ntime(int n) {
		int sum = 0;
		for (int i = 1; i <= n * 2; i++) {
			for (int j = 1; j <= n; j++) {
				sum++;
			}
		}
		for (int j = 1; j <100; j++) {
			sum++;
			sum++;
		}
	}

}
